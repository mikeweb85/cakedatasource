{
    "name": "mikeweb85/cakedatasources",
    "description": "Datasources plugin for CakePHP",
    "type": "cakephp-plugin",
	"keywords" : [
		"dsn",
		"redis",
		"predic",
		"odbc",
		"sqlserver",
		"amqp",
		"ldap"
	],
    "license": "MIT",
    "repositories": [
	    {
	        "type": "vcs",
	        "url": "git@github.com:mikeweb85/elastic-search.git"
	    }
	],
    "require": {
        "cakephp/cakephp": "^3.6|^3.7|^4.0",
        "mikeweb85/dsn": "^1.0",
        "php-amqplib/php-amqplib": "^2.8",
        "predis/predis": "^1.1",
        "sendgrid/sendgrid": "^7.4",
        "giggsey/libphonenumber-for-php": "^8.11",
        "cakephp/elastic-search": "dev-cake-4.x-declarations",
        "malkusch/lock": "^2.1",
        "react/event-loop": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "MikeWeb\\CakeSources\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MikeWeb\\CakeSources\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
